<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
		<record model="ir.ui.view" id="rh_reportwz_aqt_form_view">
            <field name="name">rh.reportwz_aqt.form</field>
            <field name="model">rh.reportwz_aqt</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
				<form string="Information:" version="7.0">
					<group>
						<field name="start_date" />
						<field name="end_date" />
						<field name="reason" colspan="2" />
						<field name="adress" colspan="2" />
						<field name="ville_id" class="oe_inline"/>
						<field name="update_adress" class="oe_inline"/>
					</group>
					<footer>
						<button type="special" special="cancel" string="Cancel" icon="gtk-yes"/>
						<button type="object" name="print_report" string="Print" icon="gtk-no" confirm="Are you sure you want to print?"/>
					</footer>
				</form>
            </field>
        </record>
