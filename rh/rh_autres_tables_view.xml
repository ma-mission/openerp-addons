	<record id="rh_ville_form_view" model="ir.ui.view">
		<field name="name">rh.ville.form</field>
		<field name="model">rh.ville</field>
		<field name="arch" type="xml">
			<form string="Villes" version="7.0">
				<notebook>
					<page string="Ville">
						<group>
							<field name="name"/>
							<field name="zip"/>
							<field name="zone"/>
							<field name="province_id"/>
						</group>
					</page>
				</notebook>
			</form>
        </field>
    </record> 
