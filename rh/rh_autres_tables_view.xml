        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="Villes" >
				<field name="name"/>
				<field name="zip"/>
				<field name="zone"/>
			</tree>
        </field>
    </record>
    <record id="rh_ville_act_view" model="ir.actions.act_window" >
        <field name="name">Villes</field>
        <field name="res_model">rh.ville</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
		<field name="help" type="html"><p class="oe_view_nocontent_create"> Click to define a new record.</p></field>
	</record>
	<menuitem name="Liste des villes" id="villes_menu"  parent="menu_autres_referentiels" action="rh_ville_act_view"/>
	
