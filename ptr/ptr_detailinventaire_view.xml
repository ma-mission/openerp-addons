<openerp>	<data>		<record id="ptr_detailinventaire_tree_view" model="ir.ui.view">            <field name="name">ptr.detailinventaire.tree</field>            <field name="model">ptr.detailinventaire</field>            <field name="arch" type="xml">                <tree string="Ligne Inventaire"  toolbar="1"  version="7.0" editable="bottom">					<field name="ptrgeo" widget="selection" />                     <field name="ptractif" widget="selection"/> 					<field name="personne" widget="selection" /> 					<field name="etat" />                     <field name="existe"/> 					<field name="observation"/>                 </tree>            </field>        </record>		<record id="ptr_detailinventaire_form_view" model="ir.ui.view">            <field name="name">ptr.detailinventaire.form</field>            <field name="model">ptr.detailinventaire</field>            <field name="arch" type="xml">                <form string="Detail Inventaire" version="7.0">					<sheet>						<group  col ="4">							<separator string ="Detail ptr actif" colspan="4"/>									<field name="ptractif" colspan="1"/> 									<field name="unite" colspan="1" widget="selection"/> 									<field name="ptrgeo" colspan="1" widget="selection"/> 									<field name="personne" colspan="1" widget="selection"/> 									<field name="etat" colspan="1"/> 									<field name="existe" help="Cocher dans le cas de l existance du patr actif" colspan="1"/> 									<field name="observation" colspan="1"/> 						</group>					</sheet>                     </form>            </field>        </record>	</data></openerp>